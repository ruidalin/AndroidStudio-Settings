<templateSet group="AndroidLog">
  <template name="Loge" value="LOG.e( &quot;$METHOD_NAME$:&quot;,&quot;$content$&quot;);" description="LOG.e(String, String)" toReformat="true" toShortenFQNames="true">
    <variable name="METHOD_NAME" expression="methodName()" defaultValue="" alwaysStopAt="false" />
    <variable name="content" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_STATEMENT" value="true" />
    </context>
  </template>
  <template name="tloge" value="Log.e( &quot;$METHOD_NAME$:&quot;,&quot;$content$&quot;);" description="Log.e(String, String)" toReformat="true" toShortenFQNames="true">
    <variable name="METHOD_NAME" expression="methodName()" defaultValue="" alwaysStopAt="false" />
    <variable name="content" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_STATEMENT" value="true" />
    </context>
  </template>
  <template name="xmlogd" value="XMLog.d($TAG$, String.format(&quot;#$METHOD_NAME$ $content$&quot;, $params$));" description="Log.d(String, String)" toReformat="true" toShortenFQNames="true">
    <variable name="TAG" expression="TAG" defaultValue="&quot;TAG&quot;" alwaysStopAt="true" />
    <variable name="METHOD_NAME" expression="methodName()" defaultValue="" alwaysStopAt="false" />
    <variable name="content" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="params" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_STATEMENT" value="true" />
    </context>
  </template>
  <template name="xmloge" value="XMLog.e(TAG, String.format(&quot;#$METHOD_NAME$ $content$&quot;, $params$));" description="Log.e(String, String)" toReformat="true" toShortenFQNames="true">
    <variable name="METHOD_NAME" expression="methodName()" defaultValue="" alwaysStopAt="false" />
    <variable name="content" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="params" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_STATEMENT" value="true" />
    </context>
  </template>
</templateSet>